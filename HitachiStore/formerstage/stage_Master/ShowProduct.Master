                        ForeColor="White" />
                    <asp:Button ID="Cancelbtn" runat="server" Height="25px" Text=" 取消" Style="background-image: url(../../image/标题头微条.jpg);
                        margin-left: 20px" BorderStyle="None" Width="75px" ForeColor="White" />
                </div>
            </center>
